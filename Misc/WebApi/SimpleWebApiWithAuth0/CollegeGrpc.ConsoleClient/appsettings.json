{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "RPCService": {
    "ServiceUrl": "https://localhost:5001"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Auth0Credentials": {
    "Domain": "Your_auth0_Domain_name",
    "Audience": "https://your-end-point",
    "client_id": "Your_Client_Id",
    "client_secret": "Your_Client_Secret",
    "grant_type": "client_credentials"
  }
}
